{
	"schemaVersion": 1,
	"id": "op-tools",
	"version": "1.7.1",
	"name": "OP Tools",
	"description": "This is a basic mod that adds op tools to survival",
	"authors": [
		"Jaymanh"
	],
	"contact": {
		"homepage": "https://github.com/jaymanh/OP-Tools-fabric",
		"sources": "https://github.com/jaymanh/OP-Tools-fabric"
	},
	"license": "CC0-1.0",
